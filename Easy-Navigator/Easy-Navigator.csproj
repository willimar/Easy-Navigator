<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Easy.Navigator</RootNamespace>
    <PackageProjectUrl>https://github.com/willimar/Easy-Navigator.git</PackageProjectUrl>
    <RepositoryUrl>https://github.com/willimar/Easy-Navigator.git</RepositoryUrl>
    <Version>1.0.1.2</Version>
    <Copyright>© 2020 Willimar</Copyright>
    <Description>Simple navigato to comunicate with REST API.</Description>
    <Product>Easy.Navigator</Product>
    <Authors>Rocha, Willimar</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
